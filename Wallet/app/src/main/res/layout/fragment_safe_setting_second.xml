<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff"
    >



    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#E8E9ED"
        />




    <View
        android:layout_marginLeft="26dp"
        android:layout_marginTop="48dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E8E9ED"
        />


    <RelativeLayout
        android:layout_marginTop="@dimen/rc_dimen_size_5"
        android:layout_marginLeft="26dp"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/rl_change_cash_pwd">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            style="@style/text_style_17sp_black1"
            android:layout_alignParentLeft="true"
            android:text="资金密码"
            />


        <ImageView
            android:id="@+id/tv_arrow_1"
            android:layout_marginRight="-10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/arrow_right"
            android:scaleY="0.6"
            android:scaleX="0.6"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="-15dp"
            android:layout_toLeftOf="@id/tv_arrow_1"
            style="@style/text_style_15sp_gray_8"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:text="修改"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="26dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E8E9ED"
        />



    <RelativeLayout
        android:layout_marginTop="@dimen/rc_dimen_size_5"
        android:layout_marginLeft="26dp"
        android:layout_width="match_parent"
        android:id="@+id/rl_change_login_pwd"
        android:layout_height="50dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            style="@style/text_style_17sp_black1"
            android:layout_alignParentLeft="true"
            android:text="登录密码"
            />


        <ImageView
            android:id="@+id/tv_arrow_2"
            android:layout_marginRight="-10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/arrow_right"
            android:scaleY="0.6"
            android:scaleX="0.6"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="-15dp"
            android:layout_toLeftOf="@id/tv_arrow_2"
            style="@style/text_style_15sp_gray_8"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:text="修改"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="26dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E8E9ED"
        />



    <RelativeLayout
        android:layout_marginTop="@dimen/rc_dimen_size_5"
        android:layout_marginLeft="26dp"
        android:layout_width="match_parent"
        android:id="@+id/rl_google_code_is_open"
        android:layout_height="50dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            style="@style/text_style_17sp_black1"
            android:layout_alignParentLeft="true"
            android:text="谷歌验证"
            />


        <ImageView
            android:id="@+id/tv_arrow_3"
            android:layout_marginRight="-10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/arrow_right"
            android:scaleY="0.6"
            android:scaleX="0.6"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="-15dp"
            android:layout_toLeftOf="@id/tv_arrow_3"
            style="@style/text_style_15sp_gray_8"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:text="未开启"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="26dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E8E9ED"
        />



</LinearLayout>
