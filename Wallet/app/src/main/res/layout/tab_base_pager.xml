<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <include
        android:id="@+id/cl_root_homepage_container"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.268"
        layout="@layout/title_bar"
        />


    <FrameLayout
        android:paddingTop="5dp"
        android:id="@+id/fl_tab_base_pager_content"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.66"
        app:layout_constraintTop_toBottomOf="@id/cl_root_homepage_container"
       >
    </FrameLayout>

</android.support.constraint.ConstraintLayout>